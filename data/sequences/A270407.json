atrix(N + 1, N + 1);", "Qget(n, g) = { if (g < 0 || g > n/2, 0, Q[n+1, g+1]) };", "Qset(n, g, v) = { Q[n+1, g+1] = v };", "Quadric({x=1}) = {", "  Qset(0, 0, x);", "  for (n = 1, length(Q)-1, for (g = 0, gmax(n),", "    my(t1 = (1+x)*(2*n-1)/3 * Qget(n-1, g),", "       t2 = (2*n-3)*(2*n-2)*(2*n-1)/12 * Qget(n-2, g-1),", "       t3 = 1/2 * sum(k = 1, n-1, sum(i = 0, g,", "       (2*k-1) * (2*(n-k)-1) * Qget(k-1, i) * Qget(n-k-1, g-i))));", "    Qset(n, g, (t1 + t2 + t3) * 6/(n+1))));", "};", "Quadric('x + O('x^(F+1)));", "concat(vector(N+2-F, n, vector(1 + gmax(n-1), g, polcoeff(Qget(n+F-2, g-1), F))))"], "xref": ["Columns k=0-1 give: A000184, A006296."], "keyword": "nonn,tabf", "offset": "2,1", "author": "_Gheorghe Coserea_, Mar 16 2016", "references": 5, "revision": 22, "time": "2018-10-18T05:11:19-04:00", "created": "2016-03-17T06:01:04-04:00"}]}